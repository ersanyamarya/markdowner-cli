{
  "name": "markdowner-cli",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "bin": {
    "markdowner": "build/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p . && npm run chmod",
    "lint": "eslint --ext ts ./src ",
    "lint:fix": "eslint --ext ts --fix ./src ",
    "test": "jest --runInBand --no-cache",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "format": "prettier --write \"**/*.{ts,ts,json}\"",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install --legacy-peer-deps",
    "chmod": "sudo chmod u+x ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersanyamarya/markdowner-cli.git"
  },
  "keywords": [],
  "author": "Sanyam Arya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ersanyamarya/markdowner-cli/issues"
  },
  "homepage": "https://github.com/ersanyamarya/markdowner-cli#readme",
  "dependencies": {
    "clear": "^0.1.0"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
